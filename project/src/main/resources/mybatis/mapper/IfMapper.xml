<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ww.project.mapper.IfMapper">
    <select id="getNewIf" resultType="com.ww.project.bean.If">
        SELECT * FROM `IF` WHERE id = (SELECT MAX(id) FROM `IF`)
    </select>
</mapper>